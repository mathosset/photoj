                <i class="fa fa-th-large"></i> Consulter mon portfolio
              </a>
            </h4>
          </div>
          <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
            <div class="row">
              <div class="spacer"></div>
              <section id="grid">
                <% data.mariage.each do |y| %>
                  <div class="col-sm-4">
                    <a href="images/<%= y.picture %>" class="swipebox" title="<%= y.caption %>">
                      <% if y.vertical == "yes" %>
                        <%= image_tag y.picture, width: "44%", class: "center-block" %>
                      <% else %>
                        <%= image_tag y.picture, width: "100%" %>
                      <% end %>
                      <div class="spacer"></div>
                    </a>
                  </div>
                <% end %>
              </section>
            </div>
